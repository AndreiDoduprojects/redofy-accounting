{
  "name": "redofy-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Redofy Accounting Monorepo",
  "workspaces": [
    "website",
    "ui-dashboard",
    "cms"
  ],
  "scripts": {
    "dev:website": "cd website && yarn dev",
    "dev:dashboard": "cd ui-dashboard && yarn dev",
    "dev:cms": "cd cms && npx directus start",
    "build:website": "cd website && yarn build",
    "build:dashboard": "cd ui-dashboard && yarn build",
    "build:cms": "cd cms/extensions && yarn build",
    "generate:website": "cd website && yarn generate",
    "generate:dashboard": "cd ui-dashboard && yarn generate",
    "test:cms": "cd cms/extensions && yarn test"
  },
  "dependencies": {
    "vue": "^3.4.0"
  },
  "packageManager": "yarn@4.9.1"
}
